{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "redirects": [
    {
      "source": "http://:path*",
      "destination": "https://:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "roastyourfriend.fun"
        }
      ],
      "destination": "https://www.roastyourfriend.fun/:path*",
      "permanent": true
    }
  ]
}