{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "/api/:match*"
    }
  ],
  "redirects": [
    {
      "source": "/:match*",
      "has": [
        {
          "type": "host",
          "value": "roastyourfriend.fun"
        }
      ],
      "destination": "https://www.roastyourfriend.fun/:match*",
      "permanent": true
    }
  ]
}