{
  "version": 2,
  "redirects": [
    {
      "source": "http://:path*",
      "destination": "https://:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "roastyourfriend.fun"
        }
      ],
      "destination": "https://www.roastyourfriend.fun/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}